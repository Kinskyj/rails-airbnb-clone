<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3 col-lg-4 col-lg-offset-4">
      <h2>Create a booking</h2>

      <%= simple_form_for([@service, @booking]) do |f| %>
        <div class="form-inputs">
          <%= f.input :date_start %>
          <%= f.input :nb_hour%>
        </div>

        <div class="form-actions">
          <%= f.button :submit %>
        </div>
      <% end %>

    </div>
  </div>
</div>
